package DAY9;


abstract class Bike
{
	

	abstract void run();
}
class Honda extends Bike
{
	void run()
	{
		System.out.println("vehicle");
	}
	void play()
	{
		System.out.println("playing");
	}
}
public class abstraction {
	public static void main(String[] args) 
	{
		Honda a=new Honda();
		a.run();
		
	}

}
